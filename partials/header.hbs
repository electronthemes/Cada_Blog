{{!-- Header Start --}}
<header class="header-area py-5 lg:py-0 bg-[#fff] {{#is "post"}}post-header{{/is}}">
    <div class="container">
        <div class="header-wrapper flex items-center justify-between gap-5">
            <a href="https://cada.com/de" class="logo">
                {{#if @site.logo}}
                    <img class="max-w-[86px]" src="{{@site.logo}}" alt="{{@site.title}}">
                    {{else}}
                    <h2 class="site-title-if-no-logo">{{@site.title}}</h2>
                {{/if}}
            </a>
            <div class="main-menu">
                <ul>
                    <li>
                        <a href="https://cada.com/de/analysis">Fertility Check</a>
                    </li>
                    <li class="relative">
                        <a href="#">Behandlungen </a>
                        <div class="mega-menu">
                            <ul>
                                <li>
                                    <a href="https://cada.com/de/treatment/ht/">
                                        <span>
                                            Hormontherapie
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Hormonelle Zyklusuntersützung</p>
                                    </a>
                                </li>
                                <li class="lg:order-none order-4">
                                    <a href="https://cada.com/de/treatment/social-freezing-male/">
                                        <span>
                                            Social Freezing Mann
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Kryokonservierung der Samenzellen</p>
                                    </a>
                                </li>
                                <li class=" lg:order-none order-2">
                                    <a href="https://cada.com/de/treatment/ivf/">
                                        <span>
                                            IVF
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>In-Vitro Fertilization</p>
                                    </a>
                                </li>
                                <li class="lg:order-none order-5">
                                    <a href="https://cada.com/de/treatment/social-freezing-female/">
                                        <span>
                                            Social Freezing Frau
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Kryokonservierung der Eizellen</p>
                                    </a>
                                </li>
                                <li class="lg:order-none order-3">
                                    <a href="https://cada.com/de/treatment/iui/">
                                        <span>
                                            IUI
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Intrauterine Insemination </p>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </li>
                    <li><a href="https://cada.com/de/why-cada/">Warum Cada</a></li>
                    <li class="relative lg:block hidden">
                        <a href="#">Mehr</a>
                        <div class="mega-menu">
                            <ul>
                                <li>
                                    <a href="https://cada.com/de/about-us/">
                                        <span>
                                            Über uns
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Erfahre mehr über das Cada-Team</p>
                                    </a>
                                </li>
                                <li>
                                    <a href="https://cadafertility.careers.flair.hr/">
                                        <span>
                                            Karriere
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Informationen zu offenen Stellen bei Cada</p>
                                    </a>
                                </li>
                                <li class=" col-span-2 max-w-[194px]">
                                    <a href="https://cada.com/de/locations/">
                                        <span>
                                            Praxis
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>Unser Standort und Einblicke in unsere Praxisräume</p>
                                    </a>
                                </li>
                                <li>
                                    <a href="https://cada.com/de/blog/">
                                        <span>
                                            Blog
                                            <span>{{> icons/menu-items-arrow}}</span>
                                        </span>
                                        <p>informationen und Artikel Zu Kinderwunsch-Themen</p>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </li>
                </ul>
                <div class="block lg:hidden mt-12 px-5 xs:px-[50px]">
                    <h6 data-v-392db25b="" class="text-base font-inter text-[#777C85] mb-4">Mehr</h6>
                    <ul class="space-y-[16px] text-lg font-medium text-dark tracking-[-.18px] leading-normal">
                        <li><a href="https://cada.com/de/about-us/">Über uns</a></li>
                        <li><a href="https://cada.com/de/locations/">Praxis</a></li>
                        <li><a href="https://cada.com/de/blog/">Blog</a></li>
                        <li><a href="https://cadafertility.careers.flair.hr/">Karriere</a></li>
                    </ul>
                </div>
            </div>
            <div class="header-right flex items-center gap-5">
                <div class="relative">
                    <div class="language-wrap flex items-center gap-[4px] py-[6px] pl-[12px] pr-[16px]  border border-[#000] h-[48px] w-[70px] rounded-[8px] cursor-pointer">
                        <div>
                            {{> icons/global}}
                        </div>
                        <span class="text-[14px] leading-[22px] tracking-[-0.14px] font-medium text-[#000] block">DE</span>
                    </div>
                    <ul class="language-selector absolute mt-6 right-0 md:right-auto md:left-1/2 md:-translate-x-1/2 max-h-60 w-[160px] overflow-auto bg-[#fff] py-2 z-30 hidden rounded-[8px] shadow-box">
                        <li class="py-2 px-6">
                            <div class="relative cursor-pointer select-none sm:text-sm">
                                <a href="https://cada.com/en/blog/" class="truncate text-start font-normal text-[14px] flex items-center gap-4 flex-wrap">
                                    <img class="h-6 w-6 rounded-full" src="{{asset "/img/flag/english.png"}}" alt="image">
                                    English
                                </a>
                            </div>
                        </li>
                        <li class="py-2 px-6 active">
                            <div class="relative cursor-pointer select-none sm:text-sm flex items-center gap-4 flex-wrap">
                                <img class="h-6 w-6 rounded-full" src="{{asset "/img/flag/deutsch.png"}}" alt="image">
                                <span class="block truncate text-start text-[14px] font-normal text-white flex-1">Deutsch</span>
                            </div>
                        </li>
                        {{!-- <li class="py-2 px-6">
                            <div class="relative cursor-pointer select-none sm:text-sm">
                                <a href="#" class="truncate text-start font-normal text-[14px] flex items-center gap-4 flex-wrap">
                                    <img class="h-6 w-6 rounded-full" src="{{asset "/img/flag/Italiano.png"}}" alt="image">
                                    Italiano
                                </a>
                            </div>
                        </li> --}}
                        
                        {{!-- <li class="py-2 px-6">
                            <div class="relative cursor-pointer select-none sm:text-sm">
                                <a href="/" class="truncate text-start font-normal text-[14px] flex items-center gap-4 flex-wrap">
                                    <img class="h-6 w-6 rounded-full" src="{{asset "/img/flag/francais.png"}}" alt="image">
                                    Français
                                </a>
                            </div>
                        </li> --}}
                    </ul>
                </div>
                <a class="btn-primary !hidden lg:!inline-block header-button" href="https://booking.cada.com/de/appointment/" target="_blank">Kostenfreie Beratung</a>
                <button class="hamgurger lg:hidden">
                    <span class="top"></span>
                    <span class="bottom"></span>
                </button>
            </div>
        </div>
    </div>
    {{!-- Progress Bar for posts --}}
    {{#is "post"}}<div class="progress-bar"></div>{{/is}}
</header>
{{!-- Header End --}}